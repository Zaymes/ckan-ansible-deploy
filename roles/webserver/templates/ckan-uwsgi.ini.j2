[uwsgi]

http            =  127.0.0.1:8080
uid             =  www-data
env             =  CKAN_INI=/usr/lib/ckan/default/ckan.ini
gid             =  www-data
wsgi-file       =  /usr/lib/ckan/default/wsgi.py
virtualenv      =  /usr/lib/ckan/default
module          =  wsgi:application
master          =  true
pidfile         =  /tmp/%n.pid
harakiri        =  50
max-requests    =  5000
vacuum          =  true
callable        =  application
strict          =  true
enable-threads  = true
lazy-apps       = true